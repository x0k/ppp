{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "check": {},
    "build": {
      "dependsOn": [
        "check",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "preview": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}