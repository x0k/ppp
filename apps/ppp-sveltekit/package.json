		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/color": "^4.2.0",
		"@types/node": "^24.10.0",
		"@vitest/browser-playwright": "^4.0.8",
		"daisyui": "^5.5.0",
		"eslint": "^9.39.1",
